local MarsLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/BlankHAX/Mars-Library/refs/heads/main/source"))()

local Window = MarsLib:MakeWindow({
    Name = "Mars Library by BlankHAX",
    HidePremium = false,
    SaveConfig = true,
    ConfigFolder = "MarsConfigs"
})

local Tab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998"
})

Tab:AddButton({
    Name = "Button Example",
    Callback = function()
        print("Clicked!")
    end
})
